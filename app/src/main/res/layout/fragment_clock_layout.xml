<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.theclock.mostaccurateclockever.fragments.ClockFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <github.ishaan.buttonprogressbar.ButtonProgressBar
        android:id="@+id/fragment_clock_button"
        style="@style/Clock.Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:type="indeterminate" />

    <com.theclock.mostaccurateclockever.views.InjectWebView
        android:id="@+id/fragment_clock_webview"
        style="@style/Clock.WebView"
        android:visibility="gone"></com.theclock.mostaccurateclockever.views.InjectWebView>

</android.support.constraint.ConstraintLayout>
